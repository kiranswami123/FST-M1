numlist1=[4,5,7,3,8]
numlist2=[9,1,2,3,6]

print ("number list1 is:",numlist1)
print ("number list2 is:",numlist2)

numlist3=[]

for num in numlist1:
 if (num % 2 !=0):
    numlist3.append(num)

for num in numlist2:
 if(num % 2 ==0):
     numlist3.append(num)


print("number list 3 is",numlist3)        
